def permutation(List,String""):
   Set = set(List)
   stringlist=[]
   if len(Set)== 1:
     String +="".join(List)
     return list([String])
   for x in Set:
     List1 = list(List)
     S = String + x
     List1.remove(xï¼‰
     stringlist.extend(permutation(List1, S))
   return stringList

string = input("Enter a string: ")
List= permutation(list(string))
print("All the permutation of gi
ven string is: " + ", ".join(List))
